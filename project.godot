SettingsManager="*res://Global/SettingsManager.gd"
GlobalFlags="*res://Global/GlobalFlags.gd"
EndingManager="*res://Global/EndingManager.gd"
SpeedrunTimer="*res://Global/SpeedrunTimer.gd"
MessageManager="*res://Global/MessageManager.gd"
GlobalStats="*res://Global/GlobalStats.gd"

[display]

